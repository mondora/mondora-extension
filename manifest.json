{
	"name": "mondora-extension",
	"manifest_version": 2,
	"description": "Import blog posts to mondora app",
	"version": "1.0.3",
	"background": {
		"scripts": [
			"js/vendor.js",
			"js/background.js"
		],
		"persistent": true
	},
	"permissions": [
		"activeTab"
	],
	"browser_action": {
		"default_icon": "img/icon-bw.png",
		"default_popup": "html/popup-loggedout.html"
	},
	"icons": {
		"128": "img/icon.png"
	}
}
